{
	"arrowParens": "always",
	"bracketSameLine": false,
	"bracketSpacing": true,
	"printWidth": 100,
	"quoteProps": "as-needed",
	"semi": true,
	"singleQuote": true,
	"tabWidth": 2,
	"trailingComma": "es5",
	"useTabs": true,
	"importOrder": ["<THIRD_PARTY_MODULES>", "^[../]", "^[./]"],
	"importOrderSeparation": true,
	"importOrderSortSpecifiers": false
}
