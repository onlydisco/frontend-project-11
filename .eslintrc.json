{
  "env": {
    "browser": true,
    "node": true
  },
  "parserOptions": {
    "ecmaVersion": "latest",
    "sourceType": "module"
  },
  "extends": ["airbnb-base", "eslint:recommended", "prettier"],
  "rules": {
    "no-param-reassign": "off",
    "import/no-unresolved": "off",
    "no-console": "off",
    "import/extensions": "off"
  }
}
